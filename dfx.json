{
  "canisters": {
    "frontend": {
      "frontend": {
        "entrypoint": "src/frontend/src/index.html"
      },
      "source": [
        "src/frontend/assets",
        "dist/frontend/"
      ],
      "type": "assets",
      "dependencies": ["main"]
    },
    "main": {
      "main": "src/backend/main.mo",
      "type": "motoko",
      "dependencies": ["items", "users", "CanDBIndex", "NacDBIndex", "personhood", "internet_identity"]
    },
    "battery": {
      "main": "src/backend/battery.mo",
      "type": "motoko",
      "dependencies": []
    },
    "users": {
      "main": "src/backend/users.mo",
      "type": "motoko",
      "dependencies": ["CanDBIndex", "NacDBIndex"]
    },
    "items": {
      "main": "src/backend/items.mo",
      "type": "motoko",
      "dependencies": ["CanDBIndex", "NacDBIndex", "call"]
    },
    "personhood": {
      "main": "src/backend/personhood.mo",
      "type": "motoko",
      "dependencies": ["ic_eth", "CanDBIndex"]
    },
    "call": {
      "main": "src/backend/http/call.mo",
      "type": "motoko"
    },
    "payments": {
      "main": "src/backend/payments.mo",
      "type": "motoko",
      "dependencies": []
    },
    "pst": {
      "main": "src/backend/pst.mo",
      "type": "motoko"
    },
    "CanDBIndex": {
      "main": "src/storage/CanDBIndex.mo",
      "type": "motoko",
      "dependencies": ["battery"]
    },
    "NacDBIndex": {
      "main": "src/storage/NacDBIndex.mo",
      "type": "motoko",
      "dependencies": ["battery"]
    },
    "CanDBPartition": {
      "main": "src/storage/CanDBPartition.mo",
      "type": "motoko",
      "deploy": false,
      "dependencies": []
    },
    "NacDBPartition": {
      "main": "src/storage/NacDBPartition.mo",
      "type": "motoko",
      "deploy": false,
      "dependencies": ["NacDBIndex"]
    },
    "ic_eth": {
      "type": "rust",
      "candid": "src/ic_eth/ic_eth.did",
      "package": "ic_eth"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "shrink": false,
      "remote": {
        "candid": "internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local": {
      "bind": "localhost:8000",
      "type": "ephemeral"
    },
    "remote": {
      "id": {
        "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai",
        "ic_eth": "jrzee-liaaa-aaaap-qhnoa-cai"
      }
    }
  },
  "metadata": [
    {
      "name": "candid:service"
    }
  ] 
}